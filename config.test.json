[
    {
        "disallowCurlyBraces": true
    },
    {
        "disallowCurlyBraces": ["while", "for", "do"]
    },
    {
        "disallowDanglingUnderscores": true
    },
    {
        "disallowDanglingUnderscores": {
            "allExcept": ["_frodo_", "__bilbo"]
        }
    },
    {
        "disallowEmptyBlocks": true
    },
    {
        "disallowFunctionDeclarations": true
    },
    {
        "requireCurlyBraces": [
            "if",
            "else",
            "for",
            "while",
            "do",
            "try",
            "catch"
        ]
    }
]
