[
    {
        "disallowCurlyBraces": true
    },
    {
        "disallowCurlyBraces": ["while", "for", "do"]
    },
    {
        "disallowDanglingUnderscores": true
    },
    {
        "disallowDanglingUnderscores": {
            "allExcept": ["_frodo_", "__bilbo"]
        }
    },
    {
        "disallowEmptyBlocks": true
    },
    {
        "disallowFunctionDeclarations": true
    },
    {
        "disallowIdentifierNames": ["foo", "bar", "temp"]
    },
    {
        "disallowImplicitTypeConversion": ["boolean", "numeric", "string", "binary"]
    },
    {
        "disallowKeywords": ["with"]
    },
    {
        "disallowKeywordsInComments": true
    },
    {
        "disallowKeywordsInComments": ["thisword"]
    },
    {
        "disallowMultipleVarDecl": true
    },
    {
        "disallowMultipleVarDecl": "strict"
    },
    {
        "disallowMultipleVarDecl": "exceptUndefined"
    },
    {
        "disallowNamedUnassignedFunctions": true
    },
    {
        "requireCurlyBraces": [
            "if",
            "else",
            "for",
            "while",
            "do",
            "try",
            "catch"
        ]
    }
]
